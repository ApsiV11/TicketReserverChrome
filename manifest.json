{
    "manifest_version":3,
    "version":"1.2.0",
    "name":"Ticket reserver",
    "description": "An extension for reserving tickets in Kide.app",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
        "https://kide.app/*"
    ]
}